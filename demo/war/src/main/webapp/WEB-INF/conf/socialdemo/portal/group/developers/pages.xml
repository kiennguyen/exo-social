<page-set
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_0 http://www.gatein.org/xml/ns/gatein_objects_1_0"
  xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_0">
  
  <page>
    <name>canvas</name>
    <title>Canvas</title>
    <access-permissions>*:/developers</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
     <portlet-application>
      <portlet>
        <application-ref>dashboard</application-ref>
        <portlet-ref>GadgetPortlet</portlet-ref>
        <preferences>
          <preference>
            <name>url</name>
            <value>http://localhost:8080/rest/jcr/repository/portal-system/production/app:gadgets/app:codeRunner/app:data/app:resources/codeRunner.xml</value>
            <read-only>false</read-only>
          </preference>
        </preferences>
      </portlet>
      <title>Canvas</title>
      <access-permissions>*:/developers</access-permissions>
      <show-info-bar>false</show-info-bar>
      <show-application-state>false</show-application-state>
      </portlet-application>
  </page>
</page-set>
