<module>
      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
    
      <!-- Inherit the default GWT style sheet.  You can change       -->
      <!-- the theme of your GWT application by uncommenting          -->
      <!-- any one of the following lines.                            -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>
      <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
      <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

      <inherits name="com.google.gwt.gadgets.Gadgets"/>
      <inherits name="com.kaboomerang.gwt.rest.REST"/>
      <inherits name="com.google.gwt.http.HTTP"/>      
      <inherits name='com.google.gwt.user.Debug'/>
      <inherits name='com.google.gwt.xml.XML'/>
                  
      <!--  Inherit the SmartGWT Toolkit library configuration. -->
      <script src="init_smartgwt_for_gadget.js" />

       <inherits name="com.smartgwt.SmartGwtNoScript"/>
<!--         
        <inherits name="com.smartgwt.SmartGwtNoScript"/>
-->      
      <!-- Specify the app entry point class. -->
      <entry-point class='org.exoplatform.gadgets.WebFileEditor.client.WebFileEditor'/>
    
      <!-- Specify the application specific style sheet. -->
      <stylesheet src='WebFileEditor.css' />
      
      <!-- Load CodeMirror editor files. -->      
      <script src="CodeMirror/js/codemirror.js"/>
      <script src="CodeMirror/js/mirrorframe.js"/>              
</module>
