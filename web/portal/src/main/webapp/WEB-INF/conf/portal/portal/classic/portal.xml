<portal-config>
  <portal-name>classic</portal-name>
  <locale>en</locale>
  <!--<factory-id>office</factory-id>-->
  <access-permissions>Everyone</access-permissions>
  <edit-permission>*:/platform/administrators</edit-permission>
  <creator>root</creator>    
    
 	<portal-layout> 		
		<application>
	    <instance-id>portal#classic:/space/BannerPortlet/banner</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
	  
	 	<application>
	  	<instance-id>portal#classic:/space/SocialNavigationPortlet/toolbar</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
		
	  <application>
	    <instance-id>portal#classic:/profile/breadcrumb/breadcrumbRandom</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
	  
	
	  <page-body> </page-body>
	  
	  <application>
	    <instance-id>portal#classic:/web/FooterPortlet/footer</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
  </portal-layout>
  
</portal-config>
