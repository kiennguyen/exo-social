<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>  

  <page>
  	<page-id>portal::classic::homepage</page-id>
    <owner-type>portal</owner-type>
    <owner-id>classic</owner-id>
    <name>homepage</name>
    <title>Home Page</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>*:/platform/administrators</edit-permission>
    <application>
      <instance-id>portal#classic:/space/HomePagePortlet/homepageportlet</instance-id>
      <title>Home Page portlet</title>
      <show-info-bar>false</show-info-bar>
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page>

  <page>
  	<page-id>portal::classic::register</page-id>
    <owner-type>group</owner-type>
    <owner-id>platform/guests</owner-id>
    <name>register</name>
    <title>Register</title>
	  <access-permissions>*:/platform/guests</access-permissions>
	  <edit-permission>*:/platform/administrators</edit-permission>
    <application>
      <instance-id>group#platform/guests:/exoadmin/AccountPortlet/Account</instance-id>
      <title>New Account</title>
      <show-info-bar>false</show-info-bar>
    </application>
  </page>

  <page>
  	<page-id>portal::classic::MyDashboard</page-id>
  	<owner-type>portal</owner-type>
    <owner-id>classic</owner-id>
    <name>MyDashboard</name>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <application>
      <instance-id>portal#classic:/dashboard/DashboardPortlet/dashboardRandom</instance-id>
      <title>dashboard</title>
      <show-info-bar>false</show-info-bar>
    </application>
  </page>

  <page>
  	<page-id>portal::classic::Profile</page-id>
  	<owner-type>portal</owner-type>
    <owner-id>classic</owner-id>
    <name>Profile</name>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <application>
      <instance-id>portal#classic:/profile/mainProfile/mainProfileRandom</instance-id>
      <title>mainProfile</title>
      <show-info-bar>false</show-info-bar>
    </application>
  </page>

  <page>
  	<page-id>portal::classic::Canvas</page-id>
  	<owner-type>portal</owner-type>
    <owner-id>classic</owner-id>
    <name>Canvas</name>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <application>
      <instance-id>portal#classic:/dashboard/GadgetPortlet/GadgetPortletRandom</instance-id>
      <title>Canvas</title>
      <!-- show-info-bar>false</show-info-bar -->
    </application>
  </page>

  <page>
  	<page-id>portal::classic::manageSpaces</page-id>
  	<owner-type>portal</owner-type>
    <owner-id>classic</owner-id>
    <name>manageSpaces</name>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <application>
      <instance-id>portal#classic:/space/ManageSpacesPortlet/managespaces</instance-id>
      <title>ManageSpaces</title>
      <show-info-bar>false</show-info-bar>
    </application>
  </page>

</page-set>
