<?xml version="1.0" encoding="ISO-8859-1"?>
<container template="system:/groovy/portal/webui/container/UIContainer.gtmpl">	
	<container template="system:/groovy/portal/webui/container/UIToolbarContainer.gtmpl">
	  <access-permissions>*:/platform/users; *:/organization/management/executive-board</access-permissions>
		<container id="StarToolBarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/exoadmin/StarToolbarPortlet/starportlet</instance-id>
	      <access-permissions>Everyone</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>			
		</container>
		<container id="MyProfileToolbarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/profile/MyProfileToolbarPortlet/myprofiletoolbarportlet</instance-id>
	      <access-permissions>Everyone</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>			
		</container>
		<container id="SitesToolbarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/profile/SitesToolbarPortlet/sitestoolbarportlet</instance-id>
	      <access-permissions>Everyone</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>			
		</container>
		<container id="SpacesToolbarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/profile/SpacesToolbarPortlet/spacestoolbarportlet</instance-id>
	      <access-permissions>Everyone</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>			
		</container>
		<container id="DashboardToolbarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/profile/DashBoardToolbarPortlet/dashboardtoolbarportlet</instance-id>
	      <access-permissions>Everyone</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>			
		</container>
		<container id="AdminToolBarPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  <application>
		    <instance-id>portal#classic:/exoadmin/AdminToolbarPortlet/adminportlet</instance-id>
	      <access-permissions>*:/platform/administrators</access-permissions>
		    <show-info-bar>false</show-info-bar>
		  </application>
		</container>		
	</container>

	<site-body/>
</container>