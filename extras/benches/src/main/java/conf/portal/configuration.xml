<?xml version="1.0" encoding="ISO-8859-1"?>
<configuration
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.exoplaform.org/xml/ns/kernel_1_0.xsd http://www.exoplaform.org/xml/ns/kernel_1_0.xsd"
   xmlns="http://www.exoplaform.org/xml/ns/kernel_1_0.xsd">

  <component>
    <type>org.exoplatform.social.benches.DataInjector</type>
  </component>

  <component>
    <type>org.exoplatform.social.benches.StartupInjectionConfig</type>
    <init-params>
      <properties-param>
        <name>inject.conf</name>
        <description>possible properties are 'people', 'relations' and 'activities'  or USERNAME.activities (ex: john.activities) as long values</description>  
      	<!--  Example config : 100 of each -->
      	<!--  
        <property name="people" value="100"/>
        <property name="relations" value="100"/>
        <property name="activities" value="100"/>
        <property name="john.activities" value="10"/>
        -->
      </properties-param>
    </init-params>
  </component>

</configuration>
