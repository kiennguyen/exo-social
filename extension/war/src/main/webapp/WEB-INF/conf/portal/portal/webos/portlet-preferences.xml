<portlet-preferences-set>
	<portlet-preferences>
    <owner-type>portal</owner-type>
    <owner-id>webos</owner-id>
    <window-id>portal#webos:/web/IFramePortlet/blog</window-id>
    <preference>
        <name>url</name>
        <value>http://blog.exoplatform.org</value>
        <read-only>false</read-only>
      </preference>
	</portlet-preferences>
	
	<portlet-preferences>
    <owner-type>portal</owner-type>
