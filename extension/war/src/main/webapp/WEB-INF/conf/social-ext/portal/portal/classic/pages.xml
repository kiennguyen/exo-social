  <page>
    <name>people</name>
    <title>Manage People</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <portlet-application>
      <portlet>
        <application-ref>profile</application-ref>
        <portlet-ref>PeoplePortlet</portlet-ref>
      </portlet>
      <title>Manage People</title>
      <access-permissions>*:/platform/users</access-permissions>
      <show-info-bar>false</show-info-bar>      
    </portlet-application>
  </page>
  
  <page>
    <name>activities</name>
    <title>Activities</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <container id="sociallayout" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
	 		<container id="ProfileNavigation" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	 		  <access-permissions>*:/platform/users</access-permissions>
	  		<portlet-application>
		     <portlet>
		       <application-ref>profile</application-ref>
		       <portlet-ref>profilenavigationportlet</portlet-ref>
		     </portlet>
		     <title>Profile Navigation</title>
		     <access-permissions>*:/platform/users</access-permissions>
		     <show-info-bar>false</show-info-bar>
		     <show-application-state>true</show-application-state>      
		   </portlet-application>
	  	</container>
	 		<container id="ProfileActivity" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	 		   <access-permissions>*:/platform/users</access-permissions>
	 				<portlet-application>
			     <portlet>
			       <application-ref>dashboard</application-ref>
			       <portlet-ref>GadgetPortlet</portlet-ref>
			       <preferences>
		          <preference>
		            <name>url</name>
				        <value>http://localhost:8080/rest/jcr/repository/portal-system/production/app:gadgets/app:StatusUpdate/app:data/app:resources/activities.xml</value>
				        <read-only>false</read-only>
		          </preference>
		        </preferences>
			     </portlet>
			     <title>Gadget Portlet</title>
			     <access-permissions>*:/platform/users</access-permissions>
			     <show-info-bar>false</show-info-bar>
			     <show-application-state>true</show-application-state>      
		   		</portlet-application>
	 		</container>
		</container>
  </page>
  
